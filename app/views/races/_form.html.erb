<%= form_for @race do |f| %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <br>
  <%= f.label :city %>
  <%= f.text_field :city %>
<br>
  <%= f.label :state %>
  <%= f.text_field :state %>
<br>
  <%= f.label :start_date %>
  <%= f.text_field :start_date %>
<br>
  <%= f.label :end_date %>
  <%= f.text_field :end_date %>
<br>
  <%= f.submit %>
<% end %>
